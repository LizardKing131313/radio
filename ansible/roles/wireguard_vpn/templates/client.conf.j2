[Interface]
PrivateKey = {{ peer_private_key }}
Address = {{ peer_address }}
{% if peer_dns %}DNS = {{ peer_dns | join(', ') }}{% endif %}

[Peer]
PublicKey = {{ wg_server_public_key }}
{% if peer_use_psk %}PresharedKey = {{ peer_preshared_key }}{% endif %}
Endpoint = {{ wg_endpoint_effective }}:{{ wg_listen_port }}
AllowedIPs = {{ peer_allowed_ips_client }}
{% if peer_keepalive is defined %}PersistentKeepalive = {{ peer_keepalive }}{% endif %}
